{
  "@quasar/dotenv": {
    "env_development": ".env",
    "env_production": "wenv",
    "common_root_object": "none",
    "create_env_files": true,
    "add_env_to_gitignore": true
  },
  "@quasar/testing": {
    "harnesses": [
      "unit-jest",
      "unit-ava",
      "e2e-cypress",
      "e2e-webdriver",
      "quality",
      "security",
      "security-antivuln"
    ]
  },
  "@quasar/testing-unit-jest": {
    "babel": "babelrc",
    "options": [
      "scripts",
      "SFC",
      "wallabyjs",
      "majestic"
    ]
  },
  "@quasar/testing-unit-ava": {
    "babel": "babelrc",
    "options": [
      "SFC",
      "wallabyjs",
      "scripts"
    ]
  },
  "@quasar/testing-e2e-cypress": {
    "options": [
      "scripts"
    ]
  },
  "@quasar/testing-quality": {
    "options": [
      "scripts"
    ]
  },
  "@quasar/testing-security": {
    "options": [
      "zap_local"
    ],
    "zapbrowser": "Firefox"
  },
  "@quasar/testing-security-antivuln": {}
}