{
  "name": "tissue",
  "version": "0.0.1",
  "description": "An issue tracker for spoderman",
  "productName": "Tissue",
  "cordovaId": "org.cordova.quasar.app",
  "capacitorId": "",
  "author": "Al Gae <algae1234567890@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore ./",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit": "ava",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "test:unit:ui": "majestic",
    "test:e2e": "cypress open -d",
    "test:e2e:CI": "cypress run",
    "serve:test:lighthouse": "quasar serve test/lighthouse/reports/ --port 8789",
    "audit:snyk": "snyk test --json > ./test/audits/snyk.json",
    "audit:node_modules": "yarn audit --json > ./test/audits/node_modules_audit.json",
    "audit:licenses": "nlf --summary detail > ./test/audits/licenses.txt",
    "audit:lighthouse": "lighthouse http://localhost:8080 view --output-path=./test/lighthouse/reports/index.html --config-path=./test/lighthouse/configs/full-config.js"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "@vue/composition-api": "^0.5.0",
    "add": "^2.0.6",
    "axios": "^0.18.1",
    "katex": "^0.11.1",
    "mermaid": "^8.5.1",
    "quasar": "^1.9.5",
    "quasar-tiptap": "^1.4.3",
    "tiptap": "^1.27.1",
    "tiptap-extensions": "^1.29.1",
    "vue-axios": "^2.1.5",
    "vue-codemirror": "^4.0.6",
    "vue-google-oauth2": "^1.5.4",
    "vue-i18n": "^8.0.0",
    "vue-social-auth": "^1.4.3",
    "yarn": "^1.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-export-default-from": "^7.10.1",
    "@babel/plugin-syntax-export-default-from": "^7.10.1",
    "@quasar/app": "^1.6.2",
    "@quasar/quasar-app-extension-dotenv": "^1.0.1",
    "@quasar/quasar-app-extension-testing": "^1.0.0",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^1.0.0-beta.11",
    "@quasar/quasar-app-extension-testing-quality": "^1.0.0-beta.8",
    "@quasar/quasar-app-extension-testing-security": "^1.0.0-alpha.8",
    "@quasar/quasar-app-extension-testing-security-antivuln": "^1.0.0-alpha.2",
    "@quasar/quasar-app-extension-testing-unit-ava": "^1.0.0-beta.12",
    "@quasar/quasar-app-extension-testing-unit-jest": "^1.0.1",
    "@types/node": "^10.17.15",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.1.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.1.2",
    "majestic": "^1.2.24",
    "vue-jest": "^3.0.0",
    "wallaby-vue-compiler": "^1.0.3"
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ],
  "resolutions": {
    "@babel/parser": "7.7.5"
  }
}
